{
  "compilerOptions": {
    "outDir": "lib",
    "target": "ES2020",
    "module": "commonjs",
    "noImplicitReturns": true,
    "sourceMap": false,
    "skipLibCheck": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules/*",
    "node_modules",
    "**/*.test.ts",
    "**/test/**/*"
  ]
}