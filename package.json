{
  "name": "pangea-extensions-firebase",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "bootstrap": "lerna exec -- npm install",
    "prepare": "npm run bootstrap",
    "build": "lerna run build",
    "clean": "lerna run --parallel clean && lerna clean",
    "generate-package-locks": "lerna exec -- npm install --save --package-lock-only",
    "generate-readmes": "lerna run --parallel generate-readme",
    "test": "lerna run test"
  },
  "optionalDependencies": {
    "@nx/nx-linux-x64-gnu": "^20.0.2"
  },
  "devDependencies": {
    "lerna": "^8.1.8"
  }
}
